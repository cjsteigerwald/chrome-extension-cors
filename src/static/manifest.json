{
	"manifest_version": 3,
	"name": "CORS Extension",
	"description": "Extensions to manipulate headers",
	"version": "1.0.0",
	"icons": {
		"16": "icon1.png",
		"48": "icon1.png",
		"128": "icon1.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_title": "CORS Extension",
		"default_icon": "icon1.png"
	},
	"permissions": [
		"storage",
		"tabs",
		"alarms",
		"scripting",
		"cookies",
		"activeTab",
		"declarativeNetRequest",
		"declarativeNetRequestFeedback"
		
	],	
	"host_permissions": ["<all_urls>"],
	"options_page": "options.html",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_start",
			"js": ["contentScript.js"]
		}
	]
}
